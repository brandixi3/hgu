<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>hgu</id>
	<name>Human Genetics Module</name>
	<version>1.0-SNAPSHOT</version>
	<package>org.openmrs.module.hgu
	</package>
	<author>DushmanE</author>
	<description>
		Helps to start writing OpenMRS modules
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/hgu/update.rdf
	</updateURL>
	<require_version>1.11.3</require_version>
	<!-- / Module Properties -->


	<!-- Module Activator -->
	<activator>org.openmrs.module.hgu.HumanGeneticsModuleActivator
	</activator>


	<require_modules>
		<require_module>
			org.openmrs.module.uiframework
		</require_module>
		<require_module>
			org.openmrs.module.appframework
		</require_module>
	</require_modules>

	<!-- Maps hibernate file's, if present -->
	<mappingFiles>

		HumanGeneticsModule.hbm.xml

	</mappingFiles>

	<!-- Internationalization -->
	<!-- All message codes should start with hgu. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

